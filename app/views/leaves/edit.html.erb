<h1>Leave actions</h1>

<%= form_for(@leave) do |f| %>

  <div class="field">
    <%= f.label :user_id, "user_id" %><br />
    <%= f.text_field :user_id %>
  </div>

  <div class="field">
    <%= f.label :status, "status" %><br />
    <%= f.check_box :status %>
  </div>

  <div class="actions">
    <%= f.submit "submit" %>
  </div>

<% end %>

<%= link_to 'Back', leaves_path %>
